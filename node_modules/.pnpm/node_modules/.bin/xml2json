#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/vincent/Documents/code/listenr-shortlinks/node_modules/.pnpm/xml2json@0.12.0/node_modules/xml2json/bin/node_modules:/home/vincent/Documents/code/listenr-shortlinks/node_modules/.pnpm/xml2json@0.12.0/node_modules/xml2json/node_modules:/home/vincent/Documents/code/listenr-shortlinks/node_modules/.pnpm/xml2json@0.12.0/node_modules:/home/vincent/Documents/code/listenr-shortlinks/node_modules/.pnpm/node_modules:/home/vincent/Documents/code/listenr-shortlinks/node_modules:/home/vincent/Documents/code/node_modules:/home/vincent/Documents/node_modules:/home/vincent/node_modules:/home/node_modules:/node_modules:/home/vincent/Documents/code/listenr-shortlinks/node_modules/.pnpm/node_modules/xml2json/bin/node_modules:/home/vincent/Documents/code/listenr-shortlinks/node_modules/.pnpm/node_modules/xml2json/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../xml2json/bin/xml2json" "$@"
else
  exec node  "$basedir/../xml2json/bin/xml2json" "$@"
fi
