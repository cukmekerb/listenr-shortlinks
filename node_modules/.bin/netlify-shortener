#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/vincent/Documents/code/listenr-shortlinks/node_modules/.pnpm/netlify-shortener@1.0.4/node_modules/netlify-shortener/dist/index.js/node_modules:/home/vincent/Documents/code/listenr-shortlinks/node_modules/.pnpm/netlify-shortener@1.0.4/node_modules/netlify-shortener/dist/node_modules:/home/vincent/Documents/code/listenr-shortlinks/node_modules/.pnpm/netlify-shortener@1.0.4/node_modules/netlify-shortener/node_modules:/home/vincent/Documents/code/listenr-shortlinks/node_modules/.pnpm/netlify-shortener@1.0.4/node_modules:/home/vincent/Documents/code/listenr-shortlinks/node_modules/.pnpm/node_modules:/home/vincent/Documents/code/listenr-shortlinks/node_modules:/home/vincent/Documents/code/node_modules:/home/vincent/Documents/node_modules:/home/vincent/node_modules:/home/node_modules:/node_modules:/home/vincent/Documents/code/listenr-shortlinks/node_modules/netlify-shortener/dist/index.js/node_modules:/home/vincent/Documents/code/listenr-shortlinks/node_modules/netlify-shortener/dist/node_modules:/home/vincent/Documents/code/listenr-shortlinks/node_modules/netlify-shortener/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../netlify-shortener/dist/index.js" "$@"
else
  exec node  "$basedir/../netlify-shortener/dist/index.js" "$@"
fi
